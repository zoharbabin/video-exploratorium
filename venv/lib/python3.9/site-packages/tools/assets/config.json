{
    "version": "2.0",
    "app_name": "<app_name>",
    "stages": {
      "dev": {
        "api_gateway_stage": "dev",
        "lambda_timeout": 60,
        "lambda_memory_size": 256,
        "environment_variables": {
          "STAGE": "dev"
        },
        "autogen_policy": true
      },
      "prod": {
        "api_gateway_stage": "prod",
        "layers": [<layers>],
        "lambda_timeout": 60,
        "lambda_memory_size": 256,
        "environment_variables": {
          "STAGE": "prod",
          "LD_LIBRARY_PATH": "/var/lang/lib:/lib64:/usr/lib64:/var/runtime:/var/runtime/lib:/var/task:/var/task/lib:/opt/lib:/var/task/chalicelib/libs"
        },
        "autogen_policy": true
      }
    }
  }